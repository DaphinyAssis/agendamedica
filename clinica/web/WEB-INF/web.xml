<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>pesquisarPaciente</servlet-name>
        <servlet-class>servlet.pesquisarPaciente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>desmarcarConsulta</servlet-name>
        <servlet-class>servlet.desmarcarConsulta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>atualizarConsulta</servlet-name>
        <servlet-class>servlet.atualizarConsulta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>agendarConsulta</servlet-name>
        <servlet-class>servlet.agendarConsulta</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>pesquisarPaciente</servlet-name>
        <url-pattern>/pesquisarPaciente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>desmarcarConsulta</servlet-name>
        <url-pattern>/desmarcarConsulta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>atualizarConsulta</servlet-name>
        <url-pattern>/atualizarConsulta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>agendarConsulta</servlet-name>
        <url-pattern>/agendarConsulta</url-pattern>
    </servlet-mapping>
</web-app>
